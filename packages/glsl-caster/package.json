      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "author": "zavalit",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.2",
    "@webglify/msdf-text": "^0.0.4",
    "jest": "^29.7.0",
    "mock-fs": "^5.2.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "glsl-tokenizer": "^2.1.5"
  }
}
