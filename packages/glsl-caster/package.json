{
  "name": "@webglify/glsl-caster",
  "version": "0.0.2",
  "description": "glsl AST pasrer, sertializer, importer",
  "main": "lib/index.js",
  "directories": {
    "dist": "lib"
  },
  "files": [
    "lib"
  ],
  "scripts": {
